@page
@model CalendarHeatingRoomPlanningUI.Pages.Forms.ModifySettingsModel
@{
    ViewData["Title"] = " Change Settings";
}

<h1>Calendar Settings</h1>
<form class="form-group" method="post">
    <div class="form-group">
        <label class="form-control-plaintext col-md-2">webCal:</label>
        <div class="col-md-10">
            <input type="url" asp-for="Settings.Calendar.ICalUrl" placeholder="iCal URL" />
        </div>
    </div>
    <div class="form-group">
        <button class="btn btn-outline-primary btn-sm" type="submit">Submit</button>
    </div>
</form>
